{
    "version" :  2,
    "builds": [
        {
            "src":"Weather_app/wsgi.py",
            "use":"@vercel/python",
            "config":{"maxLandasize" : "", "runtime": "python3.9"}
        },
    
        
        {
            "src":" build_files.sh ",
            "use":"@vercel/static-build",
            "config":{ "distDir": "staticfiles_build"}
        }
    ],
    "routes":[
        {
            "src":"/static/(.*) ",
            "dest":"/static/$1"
        },
        {
            "src":"/(.*) ",
            "dest":"Weather_app/wsgi.py"
        }
        
    
    ]




}

