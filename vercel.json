{

    "version": 2,

    "builds": [

    {

    "src": "Wether_app/wsgi.py",

    "use": " @ardnt/vercel-python-wsgi ",

    "config": { "maxLambdaSize": "15mb", "runtime": "python" }

    },

    {

    "src": "build_files.sh",

    "use": "@vercel/static",

    "config": {

    "distDir": "staticfiles"

     }

    }

    ],

    "routes": [

    {

    "src": "/static/(.*)",

    "dest": "/static/$1"

    },

    {

    "src": "/(.*)",

    "dest": "Wether_app/wsgi.py"

    }

    ]

    }

